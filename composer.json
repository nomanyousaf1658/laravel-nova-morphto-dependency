{
    "name": "appelit/laravel-nova-morphto-dependency",
    "description": "Extend Laravel Nova so that searchable MorphTo fields can be filtered by values of other fields",
    "keywords": [
        "laravel",
        "nova",
        "dependency"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0"
    },
    "autoload": {
        "psr-4": {
            "Appelit\\MorphToDependency\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Appelit\\MorphToDependency\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
